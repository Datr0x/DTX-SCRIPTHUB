-- Load Rayfield UI Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Display Loading Message
print("Loading GUI...")
print("Hello")

-- Create Main Window
local Window = Rayfield:CreateWindow({
   Name = "DTX | Script Hub",
   LoadingTitle = "DTX | Script Hub",
   LoadingSubtitle = "One of the newest and best script hubs.",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "DTX_ScriptHub"
   },
   Discord = {
      Enabled = true,
      Invite = "zYJeSekDJN",
      RememberJoins = false
   },
   KeySystem = true,
   KeySettings = {
      Title = "DTX | Script Hub",
      Subtitle = "Key System",
      Note = "Use the provided key to unlock the hub.",
      FileName = "DTX_Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Bestscripthubdtx0710"}
   }
})

-- Tabs and Sections
local ScriptsTab = Window:CreateTab("Scripts", nil)
local ScriptsSection = ScriptsTab:CreateSection("Game Scripts")

-- Notification
Rayfield:Notify({
   Title = "DTX",
   Content = "Welcome to DTX, a top-tier Roblox script hub.",
   Duration = 6.5,
   Image = 4483362458,
   Actions = {
      Ignore = {
         Name = "Okay!",
         Callback = function()
            print("User acknowledged welcome message.")
         end
      }
   }
})

-- Helper Function to Create Buttons
local function createScriptButton(name, url, async)
   ScriptsTab:CreateButton({
      Name = name,
      Callback = function()
         if async then
            loadstring(game:HttpGetAsync(url))()
         else
            loadstring(game:HttpGet(url))()
         end
      end,
   })
end

-- Add Game Scripts
