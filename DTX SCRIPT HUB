-- Save Game Instance
BuiltTab:CreateButton({
   Name = "Save Game",
   Callback = function()
      saveinstance()
   end,
})

-- Sliders
BuiltTab:CreateSlider({
   Name = "Player Speed",
   Range = {0, 100},
   Increment = 1,
   Suffix = "Speed",
   CurrentValue = 16,
   Flag = "SpeedSlider",
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
   end,
})

BuiltTab:CreateSlider({
   Name = "Player Size [Not Functional]",
   Range = {0, 100},
   Increment = 1,
   Suffix = "Size",
   CurrentValue = 16,
   Flag = "SizeSlider",
   Callback = function(Value)
      -- Note: Humanoid does not have a Size property. Placeholder only.
      print("Attempt to set size to", Value)
   end,
})

-- Credits Tab
